  </servlet>
  <servlet-mapping>
  	<servlet-name>Task</servlet-name>
  	<url-pattern>/task</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RunTaskServlet</servlet-name>
  	<url-pattern>/RunTaskServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TaskParameterServlet</servlet-name>
